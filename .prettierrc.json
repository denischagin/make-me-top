{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "singleAttributePerLine": true,
  "importOrder": [
    "^react(.*)",
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^@app/(.*)$",
    "^@entities/[a-zA-Z]+$",
    "^@entities/user/(.*)$",
    "^@entities/explorer/(.*)$",
    "^@entities/curator/(.*)$",
    "^@entities/galaxy/(.*)$",
    "^@shared/[a-zA-Z]+$",
    "^@shared/images/(.*)$",
    "^@shared/utils/(.*)$",
    "^@shared/constants/(.*)$",
    "^@widgets/(.*)$",
    "^@pages/(.*)$",
    "^(@shared/(.*)/interfaces$)|(./interfaces)$",
    "^@shared/types/(.*)$",
    "./styles",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "bracketSpacing": true
}
